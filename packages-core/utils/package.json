{
  "name": "@karinjs/utils",
  "version": "2.0.0",
  "description": "karin 工具库 - 统一的模块化工具集合",
  "keywords": [
    "utils",
    "karin"
  ],
  "license": "MIT",
  "author": "shijin",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "checkType": "tsc",
    "prepublishOnly": "pnpm run checkType",
    "pub": "npm publish --access public",
    "rm": "node -e \"require('fs').rmSync('node_modules', { recursive: true, force: true })\""
  },
  "devDependencies": {
    "@karinjs/envs": "workspace:*",
    "@karinjs/logger": "workspace:*",
    "@karinjs/store": "workspace:*",
    "@types/ini": "^4.1.1",
    "axios": "catalog:",
    "chokidar": "catalog:",
    "ini": "^6.0.0",
    "lodash": "catalog:",
    "semver": "catalog:",
    "yaml": "catalog:"
  },
  "peerDependencies": {
    "@karinjs/store": "workspace:*",
    "axios": "catalog:",
    "chokidar": "catalog:",
    "ini": "^5.0.0",
    "lodash": "catalog:",
    "semver": "catalog:",
    "yaml": "catalog:"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      },
      "./package.json": "./package.json"
    }
  }
}
