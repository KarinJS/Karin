{
  "name": "create-karin",
  "version": "1.0.0",
  "description": "CLI tool for creating Karin projects",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "create-karin": "./lib/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup --config tsup.config.ts",
    "pub": "npm publish --access public"
  },
  "keywords": [
    "karin",
    "cli",
    "create-karin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "prompts": "^2.4.2",
    "kolorist": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "lib",
    "templates"
  ]
}
