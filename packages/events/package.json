{
  "name": "@karinjs/events",
  "version": "0.0.1",
  "description": "类型安全的事件发射器，支持正则表达式事件名和命名空间",
  "keywords": [
    "events",
    "eventemitter",
    "typescript",
    "typed",
    "regexp"
  ],
  "license": "MIT",
  "author": "shijin",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "checkType": "tsc",
    "demo": "tsx src/demo.ts",
    "example:advanced": "tsx examples/advanced.ts",
    "example:basic": "tsx examples/basic.ts",
    "prepublishOnly": "pnpm run checkType",
    "pub": "npm publish --access public",
    "rm": "node -e \"require('fs').rmSync('node_modules', { recursive: true, force: true })\""
  },
  "devDependencies": {
    "ws": "catalog:"
  },
  "peerDependencies": {
    "ws": "catalog:"
  }
}
