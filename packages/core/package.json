{
  "name": "@karinjs/core",
  "version": "2.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "shijin",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./config": {
      "types": "./src/exports/config.ts",
      "default": "./src/exports/config.ts"
    },
    "./event": {
      "types": "./src/exports/event.ts",
      "default": "./src/exports/event.ts"
    }
  },
  "scripts": {
    ".": "pnpm app",
    "app": "node dist/start/index.mjs",
    "app:bun": "bun dist/start/index.mjs",
    "build": "tsdown",
    "build:dev": "tsc && tsup",
    "build:locale": "node cli/locale.js",
    "build:types": "tsc && tsup --config tsup.config.types.ts",
    "checkType": "tsc",
    "cli": "node dist/cli/index.mjs",
    "cli:dev": "tsx exports/cli/index.ts",
    "cp:locale": "tsx scripts/locale.ts",
    "dev": "cross-env EBV_FILE=\"development.env\" node --conditions=development --import tsx src/start/index.ts",
    "dev:bun": "cross-env EBV_FILE=\"development.env\" bun --conditions=development src/start/index.ts",
    "pr": "node cli/pr.js all",
    "prepublishOnly": "pnpm run checkType",
    "pub": "npm publish --access public",
    "pub-beta": "npm publish --access public --tag beta",
    "rm": "node -e \"require('fs').rmSync('node_modules', { recursive: true, force: true })\"",
    "sort": "sort-package-json && sort-json tsconfig.json",
    "sync": "curl -X PUT \"https://registry-direct.npmmirror.com/-/package/node-karin/syncs\""
  },
  "dependencies": {
    "express": "npm:@karinjs/express@1.0.3",
    "lodash": "catalog:",
    "log4js": "catalog:",
    "moment": "catalog:",
    "sqlite3": "catalog:",
    "ws": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@karinjs/adapter": "workspace:*",
    "@karinjs/plugin-adapter-console": "workspace:*",
    "@karinjs/plugin-adapter-onebot": "workspace:*",
    "@karinjs/bot": "workspace:*",
    "@karinjs/components": "workspace:*",
    "@karinjs/config": "workspace:*",
    "@karinjs/db": "workspace:*",
    "@karinjs/envs": "workspace:*",
    "@karinjs/events": "workspace:*",
    "@karinjs/logger": "workspace:*",
    "@karinjs/node-pty": "catalog:",
    "@karinjs/onebot": "workspace:*",
    "@karinjs/store": "workspace:*",
    "@karinjs/plugin": "workspace:*",
    "@karinjs/plugin-webui-network-monitor": "^1.0.3",
    "@karinjs/plugins-list": "^1.7.0",
    "@karinjs/render": "workspace:*",
    "@karinjs/server": "workspace:*",
    "@karinjs/utils": "workspace:*",
    "@karinjs/plugin-process": "workspace:*",
    "@types/express": "^5.0.1",
    "art-template": "npm:@karinjs/art-template@1.1.0",
    "axios": "catalog:",
    "chalk": "catalog:",
    "chokidar": "catalog:",
    "cross-env": "^7.0.3",
    "dotenv": "npm:@karinjs/dotenv@^1.1.2",
    "jsonwebtoken": "npm:@karinjs/jsonwebtoken@^1.1.1",
    "node-schedule": "npm:@karinjs/node-schedule@1.0.0",
    "redis": "catalog:"
  },
  "peerDependencies": {
    "@karinjs/adapter": "workspace:*",
    "@karinjs/plugin-adapter-console": "workspace:*",
    "@karinjs/plugin-adapter-onebot": "workspace:*",
    "@karinjs/bot": "workspace:*",
    "@karinjs/components": "workspace:*",
    "@karinjs/config": "workspace:*",
    "@karinjs/db": "workspace:*",
    "@karinjs/envs": "workspace:*",
    "@karinjs/events": "workspace:*",
    "@karinjs/logger": "workspace:*",
    "@karinjs/store": "workspace:*",
    "@karinjs/plugin": "workspace:*",
    "@karinjs/render": "workspace:*",
    "@karinjs/server": "workspace:*",
    "@karinjs/utils": "workspace:*",
    "@karinjs/plugin-process": "workspace:*"
  }
}