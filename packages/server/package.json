{
  "name": "@karinjs/server",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "server",
    "karinjs"
  ],
  "license": "MIT",
  "author": "shijin",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "checkType": "tsc",
    "demo": "tsx src/demo.ts",
    "example:advanced": "tsx examples/advanced.ts",
    "example:basic": "tsx examples/basic.ts",
    "prepublishOnly": "pnpm run checkType",
    "pub": "npm publish --access public",
    "rm": "node -e \"require('fs').rmSync('node_modules', { recursive: true, force: true })\""
  },
  "dependencies": {
    "jsonwebtoken": "npm:@karinjs/jsonwebtoken@^1.1.1"
  },
  "devDependencies": {
    "@karinjs/core": "workspace:*",
    "@karinjs/envs": "workspace:*",
    "@karinjs/logger": "workspace:*",
    "@karinjs/paths": "workspace:*",
    "@karinjs/plugin": "workspace:*",
    "@karinjs/utils": "workspace:*",
    "axios": "catalog:",
    "express": "catalog:",
    "moment": "catalog:",
    "ws": "catalog:"
  },
  "peerDependencies": {
    "@karinjs/core": "workspace:*",
    "@karinjs/envs": "workspace:*",
    "@karinjs/logger": "workspace:*",
    "@karinjs/paths": "workspace:*",
    "@karinjs/plugin": "workspace:*",
    "@karinjs/utils": "workspace:*",
    "axios": "catalog:",
    "express": "catalog:",
    "moment": "catalog:",
    "ws": "catalog:"
  }
}
