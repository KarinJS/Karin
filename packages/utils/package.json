{
  "name": "@karinjs/utils",
  "version": "0.0.1",
  "description": "karin 工具库",
  "keywords": [
    "utils",
    "karin"
  ],
  "license": "MIT",
  "author": "shijin",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
