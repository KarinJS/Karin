  },
  "devDependencies": {
    "@karinjs/adapter": "workspace:*",
    "@karinjs/core": "workspace:*",
    "@karinjs/logger": "workspace:",
    "@karinjs/paths": "workspace:*",
    "@karinjs/plugin": "workspace:",
    "@karinjs/server": "workspace:*",
    "@types/ini": "^4.1.1",
    "axios": "catalog:",
    "chokidar": "4.0.3",
    "ini": "^5.0.0",
    "lodash": "catalog:",
    "yaml": "catalog:"
  },
  "peerDependencies": {
    "@karinjs/adapter": "workspace:*",
    "@karinjs/core": "workspace:*",
    "@karinjs/logger": "workspace:",
    "@karinjs/paths": "workspace:*",
    "@karinjs/plugin": "workspace:",
    "@karinjs/server": "workspace:*",
    "axios": "catalog:",
    "chokidar": "4.0.3",
    "lodash": "catalog:",
    "yaml": "catalog:"
  }
