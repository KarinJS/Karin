{
  "name": "karin",
  "version": "0.1.0",
  "private": true,
  "author": "Karin",
  "type": "module",
  "imports": {
    "#Karin": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "workspaces": [
    "plugins/**"
  ],
  "scripts": {
    "app": "node .",
    "debug": "node . --dev",
    "delete": "pm2 delete ./config/config/pm2.yaml",
    "init": "node lib/tools/install.js",
    "init:dev": "node lib/tools/install.js",
    "init:pack": "node lib/tools/install.js",
    "log": "node lib/tools/pm2Log.js",
    "monit": "pm2 monit",
    "restart": "pm2 restart ./config/config/pm2.yaml",
    "sort:pack": "npx sort-package-json",
    "start": "pm2 start ./config/config/pm2.yaml && pm2 monit",
    "stop": "pm2 delete ./config/config/pm2.yaml",
    "uninstall": "node lib/tools/uninstall.js",
    "ver": "node lib/tools/updateVersion.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.10.5",
    "@grpc/proto-loader": "0.7.12",
    "art-template": "4.13.2",
    "axios": "1.6.8",
    "chalk": "5.3.0",
    "chokidar": "3.6.0",
    "express": "4.19.2",
    "kritor-proto": "^0.0.6",
    "level": "8.0.1",
    "lodash": "4.17.21",
    "log4js": "6.9.1",
    "moment": "2.30.1",
    "node-schedule": "2.1.1",
    "redis": "4.6.14",
    "ws": "8.16.0",
    "yaml": "2.4.1"
  },
  "devDependencies": {
    "eslint": "9.4.0",
    "neostandard": "^0.7.0",
    "sort-package-json": "^2.10.0"
  },
  "engines": {
    "node": ">=18"
  }
}